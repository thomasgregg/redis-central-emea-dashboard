{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "central-emea-dashboard/frontend/dist",
  "installCommand": "npm install --no-fund",
  "framework": null,
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
